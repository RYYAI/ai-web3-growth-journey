#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_typescript@5.8.2_utf-8-validate@5.0.10/node_modules/hardhat/internal/cli/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_typescript@5.8.2_utf-8-validate@5.0.10/node_modules/hardhat/internal/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_typescript@5.8.2_utf-8-validate@5.0.10/node_modules/hardhat/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_typescript@5.8.2_utf-8-validate@5.0.10/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_typescript@5.8.2_utf-8-validate@5.0.10/node_modules/hardhat/internal/cli/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_typescript@5.8.2_utf-8-validate@5.0.10/node_modules/hardhat/internal/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_typescript@5.8.2_utf-8-validate@5.0.10/node_modules/hardhat/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/hardhat@2.22.19_bufferutil@4.0.9_typescript@5.8.2_utf-8-validate@5.0.10/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
fi
