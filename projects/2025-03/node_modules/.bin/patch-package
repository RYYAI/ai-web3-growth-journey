#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/patch-package/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/node_modules:/Users/vyi/Desktop/program/node_modules:/Users/vyi/Desktop/node_modules:/Users/vyi/node_modules:/Users/node_modules:/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/patch-package/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/node_modules:/Users/vyi/Desktop/program/node_modules:/Users/vyi/Desktop/node_modules:/Users/vyi/node_modules:/Users/node_modules:/node_modules:/Users/vyi/Desktop/program/ai-web3-growth-journey/projects/2025-03/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../patch-package/index.js" "$@"
else
  exec node  "$basedir/../patch-package/index.js" "$@"
fi
